!!! 5
html
	head
		title Local Experience
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		link(rel="stylesheet", href="https://d396qusza40orc.cloudfront.net/startup%2Fcode%2Fbootstrap-combined.no-icons.min.css")
		link(rel="stylesheet", href="http://netdna.bootstrapcdn.com/font-awesome/3.0.2/css/font-awesome.css")
		link(rel="stylesheet", href="https://d396qusza40orc.cloudfront.net/startup%2Fcode%2Fsocial-buttons.css")
		script(src="https://d396qusza40orc.cloudfront.net/startup%2Fcode%2Fjquery.js")
		script(src="https://d396qusza40orc.cloudfront.net/startup%2Fcode%2Fbootstrap.js")
		link(href="http://fonts.googleapis.com/css?family=Ubuntu:300,400,500,700,300italic,400italic,500italic,700italic", rel="stylesheet", type="text/css")
		link(href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,300,400,600,700,800", rel="stylesheet", type="text/css")
		link(rel='stylesheet', href='/css/basic_style.css')

	body		
		block content
			div.navbar.navbar-fixed-top
				div.navbar-inner
					div.container-fluid
						ul#global-nav.nav.pull-left
							a(href='#').brand Control Panel
						button(id='btn-logout', class='btn btn-primary')
							i.icon-lock.icon-white
							| Sign Out
			div(class='row-fluid')
				div(class = 'span6 offset1')
					include account
				div(class = 'span4')
					div(class='row-fluid')
						div(class='span10 offset1')
							if(user.image == null || user.image == '') 
								img(src='http://1.bp.blogspot.com/_D_kMnDTdGMo/TJLQ74qF1mI/AAAAAAAAAKc/uh3wAmIJRnQ/s1600/rubberchickenlove.jpg', class='img-polaroid')
							else
								img(src=user.image, class='img-polaroid')
					div(class='row-fluid')
						div(class='span6 offset3')
							div(class='row-fluid')
								div(class='span12')
									button(id='btn-imageadd', class='btn btn-block btn-large btn-success') Add Profile Image
			include modals/alert
			include modals/confirm
		

	script(type="text/javascript", src="//api.filepicker.io/v1/filepicker.js")
	script(type='text/javascript')
		filepicker.setKey('A9WYrW1zTNe9YvWdlRiAlz');
	script(src='/vendor/jquery.min.js')
	script(src='/vendor/jquery.form.js')
	script(src='/vendor/bootstrap-modal.js')
	script(src='/vendor/bootstrap-transition.js')
	script(src='/js/views/home.js')
	script(src='/js/controllers/homeController.js')
	script(src='/js/form-validators/accountValidator.js')
	//script(src='/vendor/bootstrap.js')